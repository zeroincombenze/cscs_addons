<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="base_report_params_view" model="ir.ui.view">
            <field name="name">base.report.params.view</field>
            <field name="model">base.report.params</field>
            <field name="arch" type="xml">
            <form string="Params" version="7.0">
                <group col="4">
                </group>
                <footer>
                    <button name="print_report" string="Print" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
            </field>
        </record>

        <record id="action_base_report_params" model="ir.actions.act_window">
            <field name="name">Report Params Action Template</field>
            <field name="res_model">base.report.params</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

    </data>
</openerp>